<p id="notice"><%= notice %></p>

<p>
  <b>User name:</b>
  <%= @account.user_name %>
</p>

<p>
  <b>Deskripsi:</b>
  <%= @account.deskripsi %>
</p>

<p>
  <b>Premium:</b>
  <%= @account.premium %>
</p>

<p>
  <b>Income:</b>
  <%= @account.income %>
</p>

<p>
  <b>Ranking:</b>
  <%= @account.ranking %>
</p>

<p>
  <b>Fee:</b>
  <%= @account.fee %>
</p>

<p>
  <b>Birthday:</b>
  <%= @account.birthday %>
</p>

<p>
  <b>Login time:</b>
  <%= @account.login_time %>
</p>

<p>Contact</p>
<% @account.contacts.each do |contact| %>
<p>
<b>Title : </b>
<%= contact.title %>
</p>

<p>
<b>Phone number : </b>
<%= contact.phone_number %>
</p>

<% end %>


